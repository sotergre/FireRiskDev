#
# Makefile for firerisk_index_nas_v2_idn.c 
#

EXEDIR=${HOME}/BIN/
CC = gcc
CFLAGS = -g -ansi -pedantic
OBJS = include/allocate_binary.o\
       include/read_binary.o\
       include/write_binary.o\
       	firerisk_index_nas_v2_idn.o

firerisk_index_nas_v2_idn: ${OBJS}
	${CC} ${CFLAGS} -o $@ ${OBJS} -lm
	mv $@ $(EXEDIR)

clean:
	/bin/rm -f ${EXEC} firerisk_index_nas_v2_idn.o
