#
# Makefile for MOD35_readQC.c 
#

EXEDIR=/firecast/BIN/
CC = gcc
CFLAGS = -g -ansi -pedantic
OBJS = include/allocate_binary.o\
       include/read_binary.o\
       include/write_binary.o\
       	MOD35_readQC.o

MOD35_readQC: ${OBJS}
	${CC} ${CFLAGS} -o $@ ${OBJS} -lm
	mv $@ $(EXEDIR)

clean:
	/bin/rm -f ${EXEC} MOD35_readQC.o
