#
# Makefile for Keetch_Byram.c
#


EXEDIR=/firecast/BIN/
CC = gcc
CFLAGS = -g -ansi -pedantic
OBJS = include/allocate_binary.o\
       include/read_binary.o\
       include/write_binary.o\
       	Keetch_Byram.o

Keetch_Byram: ${OBJS}
	${CC} ${CFLAGS} -o $@ ${OBJS} -lm
	mv $@ $(EXEDIR)

clean:
	/bin/rm -f ${EXEC} Keetch_Byram.o
