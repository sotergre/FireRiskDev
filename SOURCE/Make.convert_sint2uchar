#
# Makefile for convert_sint2uchar.c 
#

EXEDIR=/firecast/BIN/
CC = gcc
CFLAGS = -g -ansi -pedantic
OBJS = include/allocate_binary.o\
       include/read_binary.o\
       include/write_binary.o\
       	convert_sint2uchar.o

convert_sint2uchar: ${OBJS}
	${CC} ${CFLAGS} -o $@ ${OBJS} -lm
	mv $@ $(EXEDIR)

clean:
	/bin/rm -f ${EXEC} convert_sint2uchar.o
