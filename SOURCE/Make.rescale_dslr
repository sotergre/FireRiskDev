#
# Makefile for rescale_dslr.c 
#

EXEDIR=/firecast/BIN/
CC = gcc
CFLAGS = -g -ansi -pedantic
OBJS = include/allocate_binary.o\
       include/read_binary.o\
       include/write_binary.o\
       	rescale_dslr.o

rescale_dslr: ${OBJS}
	${CC} ${CFLAGS} -o $@ ${OBJS} -lm
	mv $@ $(EXEDIR)

clean:
	/bin/rm -f ${EXEC} rescale_dslr.o
